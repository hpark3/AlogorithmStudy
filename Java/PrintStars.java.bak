import java.util.*;

public class PrintStars{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        
        for (int i = 0; i < a; i++) {
			System.out.println("*");
        	for (int j = i; j < b; j++) {
				System.out.println("*");
			}
		}
    }
}